{
    "name": "kaleido-identifier",
    "displayName": "Kaleido Identifier",
    "description": "Providing kaleido highlighting for identifiers.",
    "version": "0.0.6",
    "publisher": "zjhmale",
    "license": "BSD",
    "icon": "images/kaleido.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/zjhmale/vscode-kaleido-identifier.git"
    },
    "bugs": {
      "url": "https://github.com/zjhmale/vscode-kaleido-identifier/issues"  
    },
    "homepage": "https://github.com/zjhmale/vscode-kaleido-identifier",
